{
  "name": "bank-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-config.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-types": "swagger-typescript-api -r --route-types --union-enums --extract-request-params --extract-request-body --clean-output --no-client --path https://tofi.onrender.com/v3/api-docs --name index.ts -o src/app/models",
    "generate-types-local": "swagger-typescript-api -r --route-types --union-enums --extract-request-params --extract-request-body --clean-output --no-client --path http://localhost:8080/v3/api-docs --name index.ts -o src/app/models"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "jwt-decode": "^4.0.0",
    "npm": "^10.2.5",
    "onesignal-ngx": "^2.0.1",
    "prettier": "^3.1.0",
    "rxjs": "~7.8.0",
    "swagger-typescript-api": "^13.0.3",
